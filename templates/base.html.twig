		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			<!-- Cookies -->


			<script async src="https://www.googletagmanager.com/gtag/js?id=UA-156984796-23%22%3E"></script>
			<script>
				window.dataLayer = window.dataLayer || [];

				function gtag() {
					dataLayer.push(arguments);
				}
				gtag('js', new Date());

				gtag('config', 'UA-156984796-23');
			</script>
			<script async defer crossorigin="anonymous" src="https://connect.facebook.net/fr_FR/sdk.js#xfbml=1&version=v11.0"
				nonce="67bDoQHI"></script>
